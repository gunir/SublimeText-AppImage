{
    "buildinfo": [
    {
        "prerun": [
            "curl -sL https://download.sublimetext.com/sublime-text_build-4169_amd64.deb -o ~/.cache/deb2appimage/debs/sublime-text_build-4169_amd64.deb"
        ],
        "name": "sublime_text",
        "version": "linux",
        "deps": "",
        "repoarch": "amd64",
        "distrorepo": "Debian",
        "repoversion": "jessie-backports,jessie,jessie",
        "binarypath": "/usr/bin/subl",
        "desktoppath": "/usr/share/applications/sublime_text.desktop",
        "iconpath": "/opt/sublime_text/Icon/256x256/sublime-text.png",
        "usewrapper": "true",
        "postrun": [
            null
        ]
    }
